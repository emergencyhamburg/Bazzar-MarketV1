);OpenBtn.Visible=false;local CategoriesFrame=Instance.new("Frame",MainFrame);CategoriesFrame.Size=UDim2.new(0,140,1, -40);CategoriesFrame.Position=UDim2.new(0,0,0,40);CategoriesFrame.BackgroundColor3=Color3.fromRGB(15,15,35);CategoriesFrame.BorderSizePixel=0;local CategoriesLayout=Instance.new("UIListLayout",CategoriesFrame);CategoriesLayout.SortOrder=Enum.SortOrder.LayoutOrder;CategoriesLayout.Padding=UDim.new(0,5);local ContentFrame=Instance.new("Frame",MainFrame);ContentFrame.Size=UDim2.new(1, -150,1, -40);ContentFrame.Position=UDim2.new(0,150,0,40);ContentFrame.BackgroundColor3=Color3.fromRGB(20,20,60);ContentFrame.BorderSizePixel=0;local function ClearContent() for _,child in pairs(ContentFrame:GetChildren()) do if  not child:IsA("UIListLayout") then child:Destroy();end end end local ContentLayout=Instance.new("UIListLayout",ContentFrame);ContentLayout.SortOrder=Enum.SortOrder.LayoutOrder;ContentLayout.Padding=UDim.new(0,6);local function CreateCategoryButton(name) local btn=Instance.new("TextButton",CategoriesFrame);btn.Size=UDim2.new(1, -10,0,40);btn.BackgroundColor3=Color3.fromRGB(0,40,90);btn.TextColor3=Color3.new(1,1,1);btn.Font=Enum.Font.SourceSansBold;btn.TextSize=22;btn.Text=name;btn.AutoButtonColor=true;return btn;end local function CreateButton(text) local btn=Instance.new("TextButton",ContentFrame);btn.Size=UDim2.new(1, -10,0,40);btn.BackgroundColor3=Color3.fromRGB(40,40,80);btn.TextColor3=Color3.new(1,1,1);btn.Font=Enum.Font.SourceSans;btn.TextSize=20;btn.Text=text;btn.AutoButtonColor=true;return btn;end local function CreateTextBox(placeholder,defaultText) local tb=Instance.new("TextBox",ContentFrame);tb.Size=UDim2.new(1, -10,0,40);tb.BackgroundColor3=Color3.fromRGB(30,30,60);tb.TextColor3=Color3.new(1,1,1);tb.Font=Enum.Font.SourceSans;tb.TextSize=20;tb.PlaceholderText=placeholder;tb.Text=defaultText or "" ;tb.ClearTextOnFocus=false;tb.TextEditable=true;return tb;end local states={Fly=false,Aimbot=false,TeamCheck=true,NoClip=false,InfJump=false};local flySpeed=50;local flyConnection,bodyVelocity;local function StartFly() local character=LocalPlayer.Character;if  not character then return;end local hrp=character:FindFirstChild("HumanoidRootPart");if  not hrp then return;end bodyVelocity=Instance.new("BodyVelocity");bodyVelocity.MaxForce=Vector3.new(100000,100000,100000);bodyVelocity.Velocity=Vector3.new(0,0,0);bodyVelocity.Parent=hrp;flyConnection=RunService.Heartbeat:Connect(function() if  not states.Fly then bodyVelocity.Velocity=Vector3.new(0,0,0);return;end local moveVec=Vector3.new();if UserInputService:IsKeyDown(Enum.KeyCode.W) then moveVec+=workspace.CurrentCamera.CFrame.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.S) then moveVec-=workspace.CurrentCamera.CFrame.LookVector end if UserInputService:IsKeyDown(Enum.KeyCode.A) then moveVec-=workspace.CurrentCamera.CFrame.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.D) then moveVec+=workspace.CurrentCamera.CFrame.RightVector end if UserInputService:IsKeyDown(Enum.KeyCode.Space) then moveVec+=Vector3.new(0,1,0) end if (moveVec.Magnitude>0) then moveVec=moveVec.Unit * flySpeed ;end bodyVelocity.Velocity=moveVec;end);end local function StopFly() if flyConnection then flyConnection:Disconnect();flyConnection=nil;end if bodyVelocity then bodyVelocity:Destroy();bodyVelocity=nil;end end UserInputService.JumpRequest:Connect(function() if states.InfJump then local humanoid=LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") ;if humanoid then humanoid:ChangeState(Enum.HumanoidStateType.Jumping);end end end);local noclipConnection=RunService.Stepped:Connect(function() if (states.NoClip and LocalPlayer.Character) then for _,part in pairs(LocalPlayer.Character:GetChildren()) do if part:IsA("BasePart") then part.CanCollide=false;end end end end);local aimbotConnection;local function getClosestEnemy() local closestDist=math.huge;local closestPlayer=nil;for _,plr in pairs(Players:GetPlayers()) do if (plr~=LocalPlayer) then if ( not states.TeamCheck or (plr.Team~=LocalPlayer.Team)) then local character=plr.Character;if (character and character:FindFirstChild("HumanoidRootPart") and character:FindFirstChildOfClass("Humanoid") and (character.Humanoid.Health>0)) then local dist=(character.HumanoidRootPart.Position-LocalPlayer.Character.HumanoidRootPart.Position).Magnitude;if (dist<closestDist) then closestDist=dist;closestPlayer=plr;end end end end end return closestPlayer;end local function StartAimbot() aimbotConnection=RunService.RenderStepped:Connect(function() local target=getClosestEnemy();if (target and target.Character and target.Character:FindFirstChild("Head")) then local camera=workspace.CurrentCamera;camera.CFrame=CFrame.new(camera.CFrame.Position,target.Character.Head.Position);end end);end local function StopAimbot() if aimbotConnection then aimbotConnection:Disconnect();aimbotConnection=nil;end end local function ShowFly() ClearContent();local flyBtn=CreateButton((states.Fly and "Fly ON") or "Fly OFF" );flyBtn.MouseButton1Click:Connect(function() states.Fly= not states.Fly;if states.Fly then StartFly();flyBtn.Text="Fly ON";else StopFly();flyBtn.Text="Fly OFF";end end);local speedBox=CreateTextBox("Fly Speed",tostring(flySpeed));speedBox.ClearTextOnFocus=false;local validateSpeed=CreateButton("Validate Fly Speed");validateSpeed.MouseButton1Click:Connect(function() local val=tonumber(speedBox.Text);if (val and (val>0)) then flySpeed=val;end end);end local function ShowAimbot() ClearContent();local aimBtn=CreateButton((states.Aimbot and "Aimbot ON") or "Aimbot OFF" );aimBtn.MouseButton1Click:Connect(function() states.Aimbot= not states.Aimbot;if states.Aimbot then StartAimbot();aimBtn.Text="Aimbot ON";else StopAimbot();aimBtn.Text="Aimbot OFF";end end);local teamBtn=CreateButton((states.TeamCheck and "Team Check ON") or "Team Check OFF" );teamBtn.MouseButton1Click:Connect(function() states.TeamCheck= not states.TeamCheck;teamBtn.Text=(states.TeamCheck and "Team Check ON") or "Team Check OFF" ;end);local fovBox=CreateTextBox("FOV",tostring(workspace.CurrentCamera.FieldOfView));fovBox.FocusLost:Connect(function(enterPressed) local val=tonumber(fovBox.Text);if (val and (val>=1) and (val<=120)) then workspace.CurrentCamera.FieldOfView=val;else fovBox.Text=tostring(workspace.CurrentCamera.FieldOfView);end end);end local function ShowPlayer() ClearContent();local walkLabel=Instance.new("TextLabel",ContentFrame);walkLabel.Size=UDim2.new(1, -10,0,30);walkLabel.BackgroundTransparency=1;walkLabel.Text="WalkSpeed (You)";walkLabel.TextColor3=Color3.new(1,1,1);walkLabel.Font=Enum.Font.SourceSans;walkLabel.TextSize=18;local walkBox=CreateTextBox("WalkSpeed","16");walkBox.ClearTextOnFocus=false;local jumpLabel=Instance.new("TextLabel",ContentFrame);jumpLabel.Size=UDim2.new(1, -10,0,30);jumpLabel.BackgroundTransparency=1;jumpLabel.Text="JumpPower (You)";jumpLabel.TextColor3=Color3.new(1,1,1);jumpLabel.Font=Enum.Font.SourceSans;jumpLabel.TextSize=18;local jumpBox=CreateTextBox("JumpPower","50");jumpBox.ClearTextOnFocus=false;local validateBtn=CreateButton("Validate");validateBtn.MouseButton1Click:Connect(function() local ws=tonumber(walkBox.Text);local jp=tonumber(jumpBox.Text);local humanoid=LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") ;if humanoid then if (ws and (ws>=8) and (ws<=500)) then humanoid.WalkSpeed=ws;end if (jp and (jp>=10) and (jp<=500)) then humanoid.JumpPower=jp;end end end);end local function ShowTeleport() ClearContent();local tpLabel=Instance.new("TextLabel",ContentFrame);tpLabel.Size=UDim2.new(1, -10,0,30);tpLabel.BackgroundTransparency=1;tpLabel.Text="Teleport to Player";tpLabel.TextColor3=Color3.new(1,1,1);tpLabel.Font=Enum.Font.SourceSans;tpLabel.TextSize=18;local tpBox=CreateTextBox("PlayerName","");tpBox.ClearTextOnFocus=false;local tpBtn=CreateButton("Teleport");tpBtn.MouseButton1Click:Connect(function() local name=tpBox.Text;local target=Players:FindFirstChild(name);local char=LocalPlayer.Character;if (target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and char and char:FindFirstChild("HumanoidRootPart")) then char.HumanoidRootPart.CFrame=target.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0) ;end end);local bringLabel=Instance.new("TextLabel",ContentFrame);bringLabel.Size=UDim2.new(1, -10,0,30);bringLabel.BackgroundTransparency=1;bringLabel.Text="Bring Player";bringLabel.TextColor3=Color3.new(1,1,1);bringLabel.Font=Enum.Font.SourceSans;bringLabel.TextSize=18;local bringBox=CreateTextBox("PlayerName","");bringBox.ClearTextOnFocus=false;local bringBtn=CreateButton("Bring");bringBtn.MouseButton1Click:Connect(function() local name=bringBox.Text;local target=Players:FindFirstChild(name);local char=LocalPlayer.Character;if (target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") and char and char:FindFirstChild("HumanoidRootPart")) then target.Character.HumanoidRootPart.CFrame=char.HumanoidRootPart.CFrame + Vector3.new(0,5,0) ;end end);end local function ShowMisc() ClearContent();local infJumpBtn=CreateButton((states.InfJump and "Infinite Jump ON") or "Infinite Jump OFF" );infJumpBtn.MouseButton1Click:Connect(function() states.InfJump= not states.InfJump;infJumpBtn.Text=(states.InfJump and "Infinite Jump ON") or "Infinite Jump OFF" ;end);local noClipBtn=CreateButton((states.NoClip and "NoClip ON") or "NoClip OFF" );noClipBtn.MouseButton1Click:Connect(function() states.NoClip= not states.NoClip;noClipBtn.Text=(states.NoClip and "NoClip ON") or "NoClip OFF" ;end);local discordBtn=CreateButton("Discord Link (Copy)");discordBtn.MouseButton1Click:Connect(function() if Clipboard then Clipboard("https://discord.gg/HKwKfKBeSU");discordBtn.Text="Link Copied!";wait(2);discordBtn.Text="Discord Link (Copy)";end end);end local categories={{Name="Fly",Func=ShowFly},{Name="Aimbot",Func=ShowAimbot},{Name="Player",Func=ShowPlayer},{Name="Teleport",Func=ShowTeleport},{Name="Misc",Func=ShowMisc}};for _,cat in pairs(categories) do local btn=CreateCategoryButton(cat.Name);btn.MouseButton1Click:Connect(function() ShowContent=cat.Func;ShowContent();end);end categories[1].Func();ReduceBtn.MouseButton1Click:Connect(function() MainFrame.Visible=false;OpenBtn.Visible=true;end);OpenBtn.MouseButton1Click:Connect(function() MainFrame.Visible=true;OpenBtn.Visible=false;end);FullscreenBtn.MouseButton1Click:Connect(function() if  not isFullscreen then MainFrame.Size=UDim2.new(1,0,1,0);MainFrame.Position=UDim2.new(0,0,0,0);isFullscreen=true;else MainFrame.Size=UDim2.new(0,600,0,450);MainFrame.Position=UDim2.new(0.2,0,0.3,0);isFullscreen=false;end end); end
